{
  "platform": {
    "apiRuntime": "node:18"
  },
  "buildProperties": {
    "outputLocation": "out",
    "appLocation": "/",
    "appBuildCommand": "npm run build"
  },
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".json": "text/json",
    ".css": "text/css",
    ".js": "text/javascript",
    ".svg": "image/svg+xml",
    ".ico": "image/x-icon"
  }
}
